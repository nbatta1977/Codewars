def move_zeros(lst):
    
    # initialize empty lists to use:
    zerosList = []
    finalList = []
    
    # iterate over arguement list:
    for item in lst:
        # add zeros to zerosList:
        if item == 0:
            zerosList.append(item)
        # add non zeros to final list:
        if item != 0:
            finalList.append(item)
    # iterate over zerosList appending each zero to the end of the finalList:   
    for zero in zerosList:
        finalList.append(zero)
    # return the completed finalList:     
    return finalList
